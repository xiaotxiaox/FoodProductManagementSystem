">\r\n          <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n            <a-button-group>\r\n              <a-button type=\"primary\" @click=\"handleDownload(record)\">下载</a-button>\r\n              <a-popconfirm\r\n                title=\"确认删除吗?\"\r\n                @confirm=\"handleDelete(record)\">\r\n                <a-button type=\"danger\">删除</a-button>\r\n              </a-popconfirm>\r\n            </a-button-group>\r\n          </template>\r\n        </a-table>\r\n      </a-card>\r\n    </div>\r\n  </page-layout>\r\n</template>\r\n\r\n<script>\r\n  const columns = [\r\n    {\r\n      title: '文件名称',\r\n      dataIndex: 'name',\r\n      width: '24%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '上传人',\r\n      dataIndex: 'user.username',\r\n      width: '20%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '上传日期',\r\n      dataIndex: 'time',\r\n      width: '20%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '编辑',\r\n      dataIndex: 'operation',\r\n      scopedSlots: { customRender: 'operation' },\r\n      align: 'center'\r\n    }\r\n  ]\r\n   import api from '../../../api/scheduleControl'\r\n   import { getCookie } from '../../../utils/util'\r\n   import PageLayout from '../../../components/page/PageLayout'\r\n  export default {\r\n    name: 'ScheduleNoticeDetail1',\r\n    components: {\r\n      PageLayout\r\n    },\r\n     props: {\r\n      id: String\r\n    },\r\n    data () {\r\n      return {\r\n        columns,\r\n        status: {\r\n          loading: true\r\n        },\r\n        name: '',\r\n        fileList: [],\r\n        url: {},\r\n        headers: {\r\n          'X-CSRFToken': getCookie('csrftoken')\r\n        }\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getData()\r\n    },\r\n    methods: {\r\n      getData () {\r\n        this.status.loading = true\r\n        api. getScheduleNoticeFile1(this.id)\r\n          .then(data => {\r\n            this.id=data.id\r\n            this.part=data.part\r\n            this.give_time=data.give_time\r\n            this.reply_time=data.reply_time\r\n            this.fileList = data.notice_letter\r\n            this.status.loading = false\r\n          })\r\n      },\r\n      handleChange (info) {\r\n        if (info.file.status === 'done') {\r\n          this.file = info.file.response\r\n          api. createScheduleNoticeFile1(this.id, this.file)\r\n            .then(data => {\r\n              this.$notification.success({ message: '成功', description: '上传文件成功', key: 'SUCCESS' })\r\n              this.getData()\r\n            })\r\n        } else if (info.file.status === 'error') {\r\n          this.$notification.error({ message: '失败', description: '上传文件失败', key: 'ERROR' })\r\n        }\r\n      },\r\n      beforeUpload (file) {\r\n        let isLt10M = file.size / 1024 / 1024 /1024<= 10\r\n        if (!isLt10M) {\r\n          this.$message.error('文件大小请小于10GB')\r\n        }\r\n        return isLt10M\r\n      },\r\n      handleDownload (record) {\r\n        window.open(record.url)\r\n      },\r\n      handleDelete (record) {\r\n        api.deleteScheduleNoticeFile1(record.id)\r\n          .then(data => {\r\n            this.$notification.success({message: '成功', description: '成功删除文件', key: 'SUCCESS'})\r\n            this.getData()\r\n          })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/ScheduleControl/ScheduleNotice"}]}