\"\r\n              style=\"width: 100%\"\r\n              @click=\"handleCreate()\">\r\n              新建\r\n            </a-button>\r\n          </a-col>\r\n        </a-row>\r\n        <a-table\r\n          bordered\r\n          :columns=\"columns\"\r\n          :dataSource=\"planList\"\r\n          rowKey=\"id\"\r\n          :pagination=\"false\"\r\n          :loading=\"status.loading\">\r\n          <template slot=\"operation1\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'schedulePlanDetail1', params: {id: record.id}})\">查看</a-button>\r\n          </template>\r\n          <template slot=\"operation2\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'schedulePlanDetail2', params: {id: record.id}})\">查看</a-button>\r\n          </template>\r\n          <template slot=\"operation3\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"handleEdit(record)\">编辑</a-button>\r\n            <a-popconfirm\r\n              title=\"确认删除吗?\"\r\n              @confirm=\"handleDelete(record)\">\r\n              <a-button type=\"danger\">删除</a-button>\r\n            </a-popconfirm>\r\n          </template>\r\n        </a-table>\r\n      </a-card>\r\n    </page-layout>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n  const columns = [\r\n    {\r\n      title: '名称',\r\n      dataIndex: 'name',\r\n      width: '12%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '审核日期',\r\n      dataIndex: 'time',\r\n      width: '12%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '附件',\r\n      dataIndex: 'operation1',\r\n      scopedSlots: { customRender: 'operation1' },\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '审批',\r\n      dataIndex: 'operation2',\r\n      scopedSlots: { customRender: 'operation2' },\r\n      align: 'center'\r\n    },\r\n\r\n    {\r\n      title: '编辑',\r\n      dataIndex: 'operation3',\r\n      scopedSlots: { customRender: 'operation3' },\r\n      align: 'center'\r\n    }\r\n  ]\r\n   import PageLayout from '../../../components/page/PageLayout'\r\n  import SchedulePlanModal from './components/SchedulePlanModal'\r\n  import api from '../../../api/scheduleControl'\r\n\r\n  export default {\r\n    name: 'SchedulePlan',\r\n     components: {\r\n      SchedulePlanModal,\r\n       PageLayout\r\n    },\r\n    props: {\r\n      id: Number\r\n    },\r\n    data () {\r\n      return {\r\n        layout: {\r\n          'label-col': { span: 6 },\r\n          'wrapper-col': { span: 18 }\r\n        },\r\n        status:{\r\n          loading:true\r\n        },\r\n        form: this.$form.createForm(this),\r\n        plans: {},\r\n        modal: {\r\n          record: null,\r\n          visible: false,\r\n          type: '1',\r\n          plan_id: this.id\r\n        },\r\n        columns,\r\n        planList: []\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getData()\r\n    },\r\n    methods: {\r\n\r\n      getData () {\r\n        this.status.loading=true\r\n        api.getSchedulePlanList(this.$store.getters.projectSelected.id)\r\n          .then(data => {\r\n            this.planList = data\r\n            this.status.loading=false\r\n          })\r\n        api.getSchedulePlan(this.id)\r\n          .then(data => {\r\n            this.plans = data\r\n          })\r\n      },\r\n      handleClose () {\r\n        this.modal.type = ''\r\n        this.modal.record = null\r\n        this.modal.visible = false\r\n        this.getData()\r\n      },\r\n      handleCreate () {\r\n        this.modal.type = 'create'\r\n        this.modal.record = null\r\n        this.modal.visible = true\r\n      },\r\n      handleEdit (record) {\r\n        this.modal.type = 'edit'\r\n        this.modal.record = record\r\n        this.modal.visible = true\r\n      },\r\n      handleDelete (record) {\r\n        api.deleteSchedulePlan(record.id)\r\n          .then(data => {\r\n            this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n            this.getData()\r\n          })\r\n      },\r\n      parseFloat (f) {\r\n        return parseFloat(f)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/ScheduleControl/SchedulePlan"}]}