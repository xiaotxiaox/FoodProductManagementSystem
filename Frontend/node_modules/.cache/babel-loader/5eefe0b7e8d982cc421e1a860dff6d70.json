lick=\"handleProcessAuditCreate()\">\r\n            新建\r\n          </a-button>\r\n        </a-col>\r\n      </a-row>\r\n      <a-spin :spinning=\"status.listLoading\">\r\n        <a-table\r\n          bordered\r\n          :columns=\"columns\"\r\n          :dataSource=\"matterList\"\r\n          rowKey=\"matter_id\"\r\n          :pagination=\"false\">\r\n          <template slot=\"id\" slot-scope=\"text, record, index\">\r\n            <div>{{index+1}}</div>\r\n          </template>\r\n          <template slot=\"content\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'processAuditDetail', params: {id: record.matter_id}})\">\r\n              文件管理\r\n            </a-button>\r\n          </template>\r\n          <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"handleProcessAuditEdit(record)\">编辑</a-button>\r\n            <a-popconfirm\r\n              title=\"确认删除吗?\"\r\n              @confirm=\"handleProcessAuditDelete(record)\">\r\n              <a-button type=\"danger\">删除</a-button>\r\n            </a-popconfirm>\r\n          </template>\r\n        </a-table>\r\n      </a-spin>\r\n      <a-col\r\n        :xs=\"{span: 24}\"\r\n        :sm=\"{span: 20, offset: 2}\"\r\n        :xl=\"{span: 16, offset: 4}\">\r\n      </a-col>\r\n    </a-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProcessAuditModal from './components/ProcessAuditModal'\r\nimport api from '../../../../api/supervisorQuality'\r\nimport { mapGetters } from 'vuex'\r\nimport ACol from 'ant-design-vue/es/grid/Col'\r\n\r\nconst columns = [\r\n  {\r\n    title: '序号',\r\n    dataIndex: 'id',\r\n    scopedSlots: { customRender: 'id' },\r\n    width: '15%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '名称',\r\n    dataIndex: 'name',\r\n    width: '25%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '申报内容',\r\n    dataIndex: 'content',\r\n    scopedSlots: { customRender: 'content' },\r\n    width: '20%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '是否合格',\r\n    dataIndex: 'review_status',\r\n    width: '20%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '编辑',\r\n    dataIndex: 'operation',\r\n    scopedSlots: { customRender: 'operation' },\r\n    align: 'center'\r\n  }]\r\nexport default {\r\n  name: 'ProcessAuditTable',\r\n  components: {\r\n    ACol,\r\n    ProcessAuditModal\r\n  },\r\n  props: {\r\n    id: String\r\n  },\r\n  data () {\r\n    return {\r\n      status: {\r\n        listLoading: true\r\n      },\r\n      layout: {\r\n        'label-col': { span: 6 },\r\n        'wrapper-col': { span: 18 }\r\n      },\r\n      form: this.$form.createForm(this),\r\n      auditModal: {\r\n        record: null,\r\n        visible: false,\r\n        type: '1',\r\n        process_review_id: this.id\r\n      },\r\n      columns,\r\n      matterList: [],\r\n      project_id: this.projectSelected().id,\r\n      name:''\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    ...mapGetters(['projectSelected']),\r\n    getData () {\r\n      this.status.listLoading = false\r\n      this.process_review_id=this.id\r\n      console.log(this.id)\r\n      console.log(this.process_review_id)\r\n      api.getProcessAuditTableList(this.id)\r\n        .then(data => {\r\n          this.matterList = data\r\n          this.status.listLoading = false\r\n        })\r\n    },\r\n    handleProcessAuditClose () {\r\n      this.auditModal.type = ''\r\n      this.auditModal.record = null\r\n      this.auditModal.visible = false\r\n      this.getData()\r\n    },\r\n    handleProcessAuditCreate () {\r\n      this.auditModal.type = 'create'\r\n      this.auditModal.record = null\r\n      this.auditModal.visible = true\r\n    },\r\n    handleProcessAuditEdit (record) {\r\n      this.auditModal.type = 'edit'\r\n      this.auditModal.record = record\r\n      this.auditModal.visible = true\r\n    },\r\n    handleProcessAuditDelete (record) {\r\n      api.deleteProcessAuditTable(record.matter_id)\r\n        .then(data => {\r\n          this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .test {\r\n    color: #c82333;\r\n  }\r\n</style>"],"sourceRoot":"src/views/supervisorQuality/controlProcess/processAudit"}]}