Btn\"\r\n          :disabled=\"state.loginBtn\">\r\n          登陆\r\n        </a-button>\r\n      </a-form-item>\r\n    </a-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex'\r\nimport { timeFix } from '@/utils/util'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data () {\r\n    return {\r\n      form: this.$form.createForm(this),\r\n      state: {\r\n        loginBtn: false\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(['Login', 'Logout']),\r\n    handleSubmit (e) {\r\n      e.preventDefault()\r\n      this.form.validateFields((error, data) => {\r\n        if (!error) {\r\n          this.Login(data)\r\n            .then((data) => {\r\n              this.state.loginBtn = true\r\n              this.loginSuccess(data)\r\n            })\r\n            .catch(error => {\r\n              this.loginFail(error)\r\n              this.state.loginBtn = false\r\n            })\r\n        } else {\r\n          this.state.loginBtn = true\r\n          setTimeout(() => {\r\n            this.state.loginBtn = false\r\n          }, 500)\r\n        }\r\n      })\r\n    },\r\n    loginSuccess (data) {\r\n      this.$router.push({ name: 'project' })\r\n      setTimeout(() => {\r\n        this.$notification.success({\r\n          message: `欢迎`,\r\n          description: `${timeFix()} ${this.$store.getters.username}，欢迎回来`\r\n        })\r\n      }, 1000)\r\n    },\r\n    loginFail (error) {\r\n      this.$notification.error({\r\n        message: '错误',\r\n        description: error,\r\n        duration: 4\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .user-layout-login {\r\n    label {\r\n      font-size: 14px;\r\n    }\r\n\r\n    button.login-button {\r\n      padding: 0 15px;\r\n      font-size: 16px;\r\n      height: 40px;\r\n      width: 100%;\r\n    }\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/user"}]}