\n</template>\r\n\r\n<script>\r\n  const columns = [\r\n    {\r\n      title: '文件名称',\r\n      dataIndex: 'name',\r\n      width: '30%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '上传人',\r\n      dataIndex: 'user.username',\r\n      width: '15%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '上传日期',\r\n      dataIndex: 'time',\r\n      width: '20%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '合同类型',\r\n      dataIndex: 'contract_type',\r\n      width: '15%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '编辑',\r\n      dataIndex: 'operation',\r\n      scopedSlots: { customRender: 'operation' },\r\n      align: 'center'\r\n    }\r\n  ]\r\n  import api from '../../../api/implement'\r\n  import { getCookie } from '../../../utils/util'\r\n\r\n  export default {\r\n    name: 'ReviewDetail',\r\n    props: {\r\n      id: Number\r\n    },\r\n    data () {\r\n      return {\r\n        columns,\r\n        status: {\r\n          loading: false\r\n        },\r\n        name: '',\r\n        fileList: [],\r\n        file: {},\r\n        headers: {\r\n          'X-CSRFToken': getCookie('csrftoken')\r\n        }\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getData()\r\n    },\r\n    methods: {\r\n      getData () {\r\n        this.status.loading = false\r\n        api.getContractItem(this.id)\r\n          .then(data => {\r\n            this.name = data.name\r\n            this.fileList = data.claim_damage\r\n            this.status.loading = false\r\n          })\r\n      },\r\n      handleChange (info) {\r\n        if (info.file.status === 'done') {\r\n          this.file = info.file.response\r\n          api.createContractReviewFile(this.id, this.file)\r\n            .then(data => {\r\n              this.$notification.success({ message: '成功', description: '上传文件成功', key: 'SUCCESS' })\r\n              this.getData()\r\n            })\r\n        } else if (info.file.status === 'error') {\r\n          this.$notification.error({ message: '失败', description: '上传文件失败', key: 'ERROR' })\r\n        }\r\n      },\r\n      beforeUpload (file) {\r\n        let isLt10M = file.size / 1024 / 1024 /1024<= 10\r\n        if (!isLt10M) {\r\n          this.$message.error('文件大小请小于10GB')\r\n        }\r\n        return isLt10M\r\n      },\r\n      handleDownload (record) {\r\n        window.open(record.url)\r\n      },\r\n      handleDelete (record) {\r\n        api.deleteContractReviewFile(record.id)\r\n          .then(data => {\r\n            this.$notification.success({message: '成功', description: '成功删除文件', key: 'SUCCESS'})\r\n            this.getData()\r\n          })\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  .item {\r\n    margin-bottom: 16px;\r\n  }\r\n\r\n  .ant-upload {\r\n    width: 100%;\r\n  }\r\n</style>"],"sourceRoot":"src/views/implement/contract"}]}