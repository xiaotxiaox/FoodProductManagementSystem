 </a-button>\r\n        </a-col>\r\n      </a-row>\r\n      <a-spin :spinning=\"status.listLoading\">\r\n        <a-table\r\n          bordered\r\n          :columns=\"columns\"\r\n          :dataSource=\"matterList\"\r\n          rowKey=\"id\"\r\n          :pagination=\"false\">\r\n          <template slot=\"risk_id\" slot-scope=\"text, record, index\">\r\n            <div>{{index+1}}</div>\r\n          </template>\r\n          <template slot=\"files\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'RiskListDetail', params: {id: record.id}})\">\r\n              附件管理\r\n            </a-button>\r\n          </template>\r\n          <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"handleRiskListEdit(record)\">编辑</a-button>\r\n            <a-popconfirm\r\n              title=\"确认删除吗?\"\r\n              @confirm=\"handleRiskListDelete(record)\">\r\n              <a-button type=\"danger\">删除</a-button>\r\n            </a-popconfirm>\r\n          </template>\r\n        </a-table>\r\n      </a-spin>\r\n      <a-col\r\n        :xs=\"{span: 24}\"\r\n        :sm=\"{span: 20, offset: 2}\"\r\n        :xl=\"{span: 16, offset: 4}\">\r\n      </a-col>\r\n    </a-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RiskListModal from './components/RiskListModal'\r\nimport api from '../../../api/supervisorSecurity'\r\nimport { mapGetters } from 'vuex'\r\nimport ACol from 'ant-design-vue/es/grid/Col'\r\nimport ARow from 'ant-design-vue/es/grid/Row'\r\n\r\nconst columns = [\r\n  {\r\n    title: '序号',\r\n    dataIndex: 'risk_id',\r\n    scopedSlots: { customRender: 'risk_id' },\r\n    width: '10%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '内容',\r\n    dataIndex: 'risk_content',\r\n    width: '20%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '部位',\r\n    dataIndex: 'part',\r\n    width: '15%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '附件',\r\n    dataIndex: 'files',\r\n    scopedSlots: { customRender: 'files' },\r\n    width: '15%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '风险等级',\r\n    dataIndex: 'risk_rank',\r\n    width: '18%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '编辑',\r\n    dataIndex: 'operation',\r\n    scopedSlots: { customRender: 'operation' },\r\n    align: 'center'\r\n  }]\r\nexport default {\r\n  name: 'RiskList',\r\n  components: {\r\n    ARow,\r\n    ACol,\r\n    RiskListModal\r\n  },\r\n  props: {\r\n    id: Number\r\n  },\r\n  data () {\r\n    return {\r\n      status: {\r\n        listLoading: true,\r\n        tableLoading: true\r\n      },\r\n      layout: {\r\n        'label-col': { span: 6 },\r\n        'wrapper-col': { span: 18 }\r\n      },\r\n      form: this.$form.createForm(this),\r\n      riskModal: {\r\n        record: null,\r\n        visible: false,\r\n        type: '1',\r\n        id: this.id\r\n      },\r\n      columns,\r\n      matterList: [],\r\n      project_id: this.projectSelected().id,\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    ...mapGetters(['projectSelected']),\r\n    getData () {\r\n      this.status.listLoading = false\r\n      api.getRiskList(this.project_id)\r\n        .then(data => {\r\n          this.matterList = data\r\n          this.status.listLoading = false\r\n        })\r\n    },\r\n    handleRiskListClose () {\r\n      this.riskModal.type = ''\r\n      this.riskModal.record = null\r\n      this.riskModal.visible = false\r\n      this.getData()\r\n    },\r\n    handleRiskListCreate () {\r\n      this.riskModal.type = 'create'\r\n      this.riskModal.record = null\r\n      this.riskModal.visible = true\r\n    },\r\n    handleRiskListEdit (record) {\r\n      this.riskModal.type = 'edit'\r\n      this.riskModal.record = record\r\n      this.riskModal.visible = true\r\n    },\r\n    handleRiskListDelete (record) {\r\n      api.deleteRiskList(record.id)\r\n        .then(data => {\r\n          this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .test {\r\n    color: #c82333;\r\n  }\r\n</style>"],"sourceRoot":"src/views/supervisorSecurity/riskList"}]}