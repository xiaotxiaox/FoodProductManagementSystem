al.record\"\r\n      :visible=\"modal.visible\"\r\n      :type=\"modal.type\"\r\n      :detail_id=\"modal.detail_id\"\r\n      v-if=\"modal.visible\"\r\n      @close=\"handleClose()\">\r\n      </process-detail-modal>\r\n        <a-card style=\"margin-bottom: 16px\">\r\n      <a-row class=\"item\">\r\n        <a-col\r\n          :xs=\"{span: 24}\"\r\n          :sm=\"{span: 12}\"\r\n          :xl=\"{span: 20}\">\r\n          <span style=\"font-size: 20px; margin-left: 4px\">{{details.title}}</span>\r\n        </a-col>\r\n        <a-col\r\n          :xs=\"{ span: 24 }\"\r\n          :sm=\"{ span: 12, offset: 12 }\"\r\n          :xl=\"{ span: 4, offset: 20 }\">\r\n          <a-button\r\n            type=\"primary\"\r\n            icon=\"plus\"\r\n            style=\"float: right; margin-right: 5px\"\r\n            @click=\"handleCreate()\">\r\n            新建\r\n          </a-button>\r\n           <a-button\r\n            style=\"float: right; margin-right: 5px;\"\r\n            @click=\"$router.go(-1)\">\r\n            返回\r\n          </a-button>\r\n        </a-col>\r\n\r\n      </a-row>\r\n      <a-table\r\n        bordered\r\n        :columns=\"columns\"\r\n        :dataSource=\"detailList\"\r\n        rowKey=\"id\"\r\n        :pagination=\"false\">\r\n        <template slot=\"getName\" slot-scope=\"text, record, index\">\r\n          {{getName(record)}}\r\n        </template>\r\n        <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n          <a-button @click=\"handleEdit(record)\">编辑</a-button>\r\n          <a-popconfirm\r\n            title=\"确认删除吗?\"\r\n            @confirm=\"handleDelete(record)\">\r\n            <a-button type=\"danger\">删除</a-button>\r\n          </a-popconfirm>\r\n        </template>\r\n      </a-table>\r\n    </a-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n  const columns = [\r\n    {\r\n      title: '支付时间',\r\n      dataIndex: 'time',\r\n      width: '12%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '本次实付',\r\n      dataIndex: 'money',\r\n      width: '12%',\r\n      align: 'center'\r\n    },\r\n    {\r\n      title: '编辑',\r\n      dataIndex: 'operation',\r\n      scopedSlots: { customRender: 'operation' },\r\n      align: 'center'\r\n    }\r\n  ]\r\n  import ProcessDetailModal from './components/processDetailModal'\r\n  import api from '../../../../api/implement'\r\n  export default {\r\n    name: 'AccountProcessDetail',\r\n    components: { ProcessDetailModal },\r\n    props: {\r\n      id: Number\r\n    },\r\n    data () {\r\n      return {\r\n        layout: {\r\n          'label-col': { span: 6 },\r\n          'wrapper-col': { span: 18 }\r\n        },\r\n        status:{\r\n          loading:true\r\n        },\r\n        form: this.$form.createForm(this),\r\n        details:{},\r\n        modal: {\r\n          record: null,\r\n          visible: false,\r\n          type: '1',\r\n          detail_id: this.id\r\n        },\r\n        columns,\r\n        detailList: []\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getData()\r\n    },\r\n    methods: {\r\n\r\n      getData () {\r\n        this.status.loading=true\r\n        this.modal.detail_id= this.id\r\n        console.log(this.modal.detail_id)\r\n        api.getInvestProcessDetailList(this.id)\r\n          .then(data => {\r\n            console.log(data)\r\n            this.name=data.name\r\n            this.style=data.style\r\n            this.project_name=data.project_name\r\n            this.contract_money=data.contract_money\r\n            this.true_money=data.true_money\r\n            this.should_money=data.should_money\r\n            this.detailList = data.child\r\n            this.status.loading=false\r\n          })\r\n        //this.$store.getters.projectSelected.id\r\n        // api.getInvestProcessDetail(this.id)\r\n        //   .then(data => {\r\n        //     console.log(data)\r\n        //     this.details = data\r\n        //   })\r\n      },\r\n      handleClose () {\r\n        this.modal.type = ''\r\n        this.modal.record = null\r\n        this.modal.visible = false\r\n        this.getData()\r\n      },\r\n      handleCreate () {\r\n        this.modal.type = 'create'\r\n        this.modal.record = null\r\n        this.modal.visible = true\r\n      },\r\n      handleEdit (record) {\r\n        this.modal.type = 'edit'\r\n        this.modal.record = record\r\n        this.modal.visible = true\r\n      },\r\n      handleDelete (record) {\r\n        api.deleteInvestProcessDetail(record.id)\r\n          .then(data => {\r\n            this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n            this.getData()\r\n          })\r\n      },\r\n      getName(record) {\r\n       let name=record.name\r\n        if (name) {\r\n          return `name`\r\n        } else {\r\n          return ''\r\n        }\r\n      },\r\n      parseFloat (f) {\r\n        return parseFloat(f)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/implement/invest/invest_account"}]}