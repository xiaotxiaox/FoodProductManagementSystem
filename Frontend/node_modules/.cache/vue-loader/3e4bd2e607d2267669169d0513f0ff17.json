pe\"\r\n      :table_id=\"noticeModal.id\"\r\n      v-if=\"noticeModal.visible\"\r\n      @close=\"handleProcessCheckNoticeClose()\">\r\n    </notice-modal>\r\n    <a-card style=\"margin-bottom: 16px\">\r\n      <a-row style=\"margin-bottom: 5px\">\r\n        <a-col\r\n          class=\"item\"\r\n          :xs=\"{ span: 24 }\"\r\n          :sm=\"{ span: 12 ,offset:12}\"\r\n          :xl=\"{ span: 4 ,offset:20}\">\r\n          <a-button\r\n            type=\"primary\"\r\n            icon=\"plus\"\r\n            style=\"width: 100%\"\r\n            @click=\"handleProcessCheckNoticeCreate()\">\r\n            新建\r\n          </a-button>\r\n        </a-col>\r\n      </a-row>\r\n      <a-spin :spinning=\"status.listLoading\">\r\n        <a-table\r\n          bordered\r\n          :columns=\"columns\"\r\n          :dataSource=\"matterList\"\r\n          rowKey=\"id\"\r\n          :pagination=\"false\">\r\n          <template slot=\"notice_letter\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'processCheckNoticeFileDetail', params: {id: record.id}})\">\r\n              文件管理\r\n            </a-button>\r\n          </template>\r\n          <template slot=\"reply_letter\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"$router.push({'name': 'processCheckReplyFileDetail', params: {id: record.id}})\">\r\n              文件管理\r\n            </a-button>\r\n          </template>\r\n          <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n            <a-button @click=\"handleProcessCheckNoticeEdit(record)\">编辑</a-button>\r\n            <a-popconfirm\r\n              title=\"确认删除吗?\"\r\n              @confirm=\"handleProcessCheckNoticeDelete(record)\">\r\n              <a-button type=\"danger\">删除</a-button>\r\n            </a-popconfirm>\r\n          </template>\r\n        </a-table>\r\n      </a-spin>\r\n      <a-col\r\n        :xs=\"{span: 24}\"\r\n        :sm=\"{span: 20, offset: 2}\"\r\n        :xl=\"{span: 16, offset: 4}\">\r\n      </a-col>\r\n    </a-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NoticeModal from './components/NoticeModal'\r\nimport api from '../../../../api/supervisorSecurity'\r\nimport { mapGetters } from 'vuex'\r\nimport ACol from 'ant-design-vue/es/grid/Col'\r\n\r\nconst columns = [\r\n  {\r\n    title: '通知单编号',\r\n    dataIndex: 'id',\r\n    width: '6%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '部位',\r\n    dataIndex: 'part',\r\n    width: '9%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '发出日期',\r\n    dataIndex: 'give_time',\r\n    width: '16%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '回复日期',\r\n    dataIndex: 'reply_time',\r\n    width: '16%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '通知单',\r\n    dataIndex: 'notice_letter',\r\n    scopedSlots: { customRender: 'notice_letter' },\r\n    width: '20%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '回复单',\r\n    dataIndex: 'reply_letter',\r\n    scopedSlots: { customRender: 'reply_letter' },\r\n    width: '20%',\r\n    align: 'center'\r\n  },\r\n  {\r\n    title: '编辑',\r\n    dataIndex: 'operation',\r\n    scopedSlots: { customRender: 'operation' },\r\n    align: 'center'\r\n  }]\r\nexport default {\r\n  name: 'processCheckNotice',\r\n  components: {\r\n    ACol,\r\n    NoticeModal\r\n  },\r\n  props: {\r\n    id: Number\r\n  },\r\n  data () {\r\n    return {\r\n      status: {\r\n        listLoading: true,\r\n        tableLoading: true\r\n      },\r\n      layout: {\r\n        'label-col': { span: 6 },\r\n        'wrapper-col': { span: 18 }\r\n      },\r\n      form: this.$form.createForm(this),\r\n      noticeModal: {\r\n        record: null,\r\n        visible: false,\r\n        type: '1',\r\n        id: this.id\r\n      },\r\n      columns,\r\n      matterList: [],\r\n      project_id: this.projectSelected().id,\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getData()\r\n  },\r\n  methods: {\r\n    ...mapGetters(['projectSelected']),\r\n    getData () {\r\n      this.status.listLoading = false\r\n      api.getProcessCheckNoticeList(this.project_id)\r\n        .then(data => {\r\n          this.matterList = data\r\n          this.status.listLoading = false\r\n        })\r\n    },\r\n    handleProcessCheckNoticeClose () {\r\n      this.noticeModal.type = ''\r\n      this.noticeModal.record = null\r\n      this.noticeModal.visible = false\r\n      this.getData()\r\n    },\r\n    handleProcessCheckNoticeCreate () {\r\n      this.noticeModal.type = 'create'\r\n      this.noticeModal.record = null\r\n      this.noticeModal.visible = true\r\n    },\r\n    handleProcessCheckNoticeEdit (record) {\r\n      this.noticeModal.type = 'edit'\r\n      this.noticeModal.record = record\r\n      this.noticeModal.visible = true\r\n    },\r\n    handleProcessCheckNoticeDelete (record) {\r\n      api.deleteProcessCheckNotice(record.id)\r\n        .then(data => {\r\n          this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .test {\r\n    color: #c82333;\r\n  }\r\n</style>"]}]}