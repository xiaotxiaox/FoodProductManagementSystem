Submit\">\r\n      <a-form-item style=\"margin-top: 24px; text-align: center; font-size: 20px;\">\r\n        用户登录\r\n      </a-form-item>\r\n      <a-form-item style=\"margin-top: 40px\">\r\n        <a-input\r\n          size=\"large\"\r\n          type=\"text\"\r\n          autocomplete=\"true\"\r\n          placeholder=\"用户名\"\r\n          v-decorator=\"[\r\n            'username',\r\n            {rules: [{ required: true, message: '请输入用户名' }], validateTrigger: 'blur'}\r\n          ]\">\r\n          <a-icon slot=\"prefix\" type=\"user\" :style=\"{ color: 'rgba(0,0,0,.25)' }\"/>\r\n        </a-input>\r\n      </a-form-item>\r\n      <a-form-item>\r\n        <a-input\r\n          size=\"large\"\r\n          type=\"password\"\r\n          autocomplete=\"true\"\r\n          placeholder=\"密码\"\r\n          v-decorator=\"[\r\n            'password',\r\n            {rules: [{ required: true, message: '请输入密码' }], validateTrigger: 'blur'}\r\n          ]\">\r\n          <a-icon slot=\"prefix\" type=\"lock\" :style=\"{ color: 'rgba(0,0,0,.25)' }\"/>\r\n        </a-input>\r\n      </a-form-item>\r\n      <a-form-item style=\"margin-top:24px\">\r\n        <a-button\r\n          size=\"large\"\r\n          type=\"primary\"\r\n          htmlType=\"submit\"\r\n          class=\"login-button\"\r\n          :loading=\"state.loginBtn\"\r\n          :disabled=\"state.loginBtn\">\r\n          登陆\r\n        </a-button>\r\n      </a-form-item>\r\n    </a-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions } from 'vuex'\r\nimport { timeFix } from '@/utils/util'\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data () {\r\n    return {\r\n      form: this.$form.createForm(this),\r\n      state: {\r\n        loginBtn: false\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(['Login', 'Logout']),\r\n    handleSubmit (e) {\r\n      e.preventDefault()\r\n      this.form.validateFields((error, data) => {\r\n        if (!error) {\r\n          this.Login(data)\r\n            .then((data) => {\r\n              this.state.loginBtn = true\r\n              this.loginSuccess(data)\r\n            })\r\n        } else {\r\n          this.state.loginBtn = true\r\n          setTimeout(() => {\r\n            this.state.loginBtn = false\r\n          }, 500)\r\n        }\r\n      })\r\n    },\r\n    loginSuccess (data) {\r\n      this.$router.push({ name: 'project' })\r\n      setTimeout(() => {\r\n        this.$notification.success({\r\n          message: `欢迎`,\r\n          description: `${timeFix()} ${this.$store.getters.username}，欢迎回来`\r\n        })\r\n      }, 1000)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  .user-layout-login {\r\n    label {\r\n      font-size: 14px;\r\n    }\r\n\r\n    button.login-button {\r\n      padding: 0 15px;\r\n      font-size: 16px;\r\n      height: 40px;\r\n      width: 100%;\r\n    }\r\n  }\r\n</style>\r\n"]}]}