;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0PA;AACA;AACA","file":"ApplicationDetail.vue","sourceRoot":"src/views/implement/application","sourcesContent":["<template>\r\n  <div>\r\n    <application-detail-modal\r\n      :record=\"modal.record\"\r\n      :visible=\"modal.visible\"\r\n      :type=\"modal.type\"\r\n      :decision_id=\"modal.decision_id\"\r\n      :application_id=\"modal.application_id\"\r\n      v-if=\"modal.visible\"\r\n      @close=\"handleClose()\">\r\n    </application-detail-modal>\r\n    <a-card style=\"margin-bottom: 16px\">\r\n      <a-row>\r\n        <a-col\r\n          :xs=\"{span: 24}\"\r\n          :sm=\"{span: 12}\"\r\n          :xl=\"{span: 20}\">\r\n          <span style=\"font-size: 20px; margin-left: 4px\">{{application.title}}</span>\r\n        </a-col>\r\n        <a-col\r\n          class=\"item\"\r\n          :xs=\"{ span: 24 }\"\r\n          :sm=\"{ span: 12 }\"\r\n          :xl=\"{ span: 4 }\">\r\n          <a-button\r\n            type=\"primary\"\r\n            icon=\"plus\"\r\n            style=\"width: 100%\"\r\n            @click=\"handleCreate()\">\r\n            新建\r\n          </a-button>\r\n        </a-col>\r\n      </a-row>\r\n      <a-table\r\n        :columns=\"columns\"\r\n        :dataSource=\"matterList\"\r\n        rowKey=\"matter_id\"\r\n        :pagination=\"false\">\r\n        <template slot=\"operation\" slot-scope=\"text, record, index\">\r\n          <a-button @click=\"handleEdit(record)\">编辑</a-button>\r\n          <a-popconfirm\r\n            title=\"确认删除吗?\"\r\n            @confirm=\"handleDelete(record)\">\r\n            <a-button type=\"danger\">删除</a-button>\r\n          </a-popconfirm>\r\n        </template>\r\n      </a-table>\r\n    </a-card>\r\n    <a-card>\r\n      <a-col\r\n        :xs=\"{span: 24}\"\r\n        :sm=\"{span: 20}\"\r\n        :xl=\"{span: 16}\">\r\n        <a-form\r\n          :form=\"form\">\r\n          <a-form-item\r\n            label=\"预计缴费\"\r\n            v-bind=\"layout\">\r\n            <a-input\r\n              type=\"number\"\r\n              addonAfter=\"万元\"\r\n              v-decorator=\"[\r\n              'estimate',\r\n              {initialValue: application.estimate ? parseFloat(application.estimate) : null}]\r\n              \">\r\n            </a-input>\r\n          </a-form-item>\r\n          <a-form-item\r\n            label=\"实际缴费\"\r\n            v-bind=\"layout\">\r\n            <a-input\r\n              type=\"number\"\r\n              addonAfter=\"万元\"\r\n              v-decorator=\"[\r\n              'actual',\r\n              {initialValue: application.actual ? parseFloat(application.actual) : null}]\r\n              \">\r\n            </a-input>\r\n          </a-form-item>\r\n          <a-form-item\r\n            label=\"目前状态\"\r\n            v-bind=\"layout\">\r\n            <a-radio-group\r\n              v-decorator=\"[\r\n              'status',\r\n              {initialValue: application.status}]\">\r\n              <a-radio value=\"DONE\">完成</a-radio>\r\n              <a-radio value=\"DOING\">正在办理</a-radio>\r\n              <a-radio value=\"CANNOT\">无法办理</a-radio>\r\n              <a-radio value=\"WILLDO\">尚未办理</a-radio>\r\n              <a-radio value=\"NEEDNOT\">不用办理</a-radio>\r\n            </a-radio-group>\r\n          </a-form-item>\r\n          <a-form-item>\r\n            <a-button\r\n              style=\"float: right\"\r\n              type=\"primary\"\r\n              @click=\"handleSubmit()\">\r\n              保存\r\n            </a-button>\r\n            <a-button\r\n              style=\"float: right; margin-right: 8px\"\r\n              @click=\"$router.go(-1)\">\r\n              返回\r\n            </a-button>\r\n          </a-form-item>\r\n        </a-form>\r\n      </a-col>\r\n    </a-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\n  const columns = [\r\n    {\r\n      title: '办理事项',\r\n      dataIndex: 'matter',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '份数',\r\n      dataIndex: 'number',\r\n      width: '8%'\r\n    }, {\r\n      title: '办理部门',\r\n      dataIndex: 'department',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '状态',\r\n      dataIndex: 'status',\r\n      width: '8%'\r\n    },\r\n    {\r\n      title: '联系电话',\r\n      dataIndex: 'telephone',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '是否缴费',\r\n      dataIndex: 'paid_status',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '办理时限',\r\n      dataIndex: 'deal_time',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '备注',\r\n      dataIndex: 'note',\r\n      width: '12%'\r\n    },\r\n    {\r\n      title: '下载',\r\n      dataIndex: 'download',\r\n      width: '13%'\r\n    },\r\n    {\r\n      title: '编辑',\r\n      dataIndex: 'operation',\r\n      scopedSlots: { customRender: 'operation' }\r\n    }\r\n  ]\r\n  import ApplicationDetailModal from './components/ApplicationDetailModal'\r\n  import api from '../../../api/implement'\r\n\r\n  export default {\r\n    name: 'ApplicationDetail',\r\n    components: {\r\n      ApplicationDetailModal\r\n    },\r\n    props: {\r\n      id: Number\r\n    },\r\n    data () {\r\n      return {\r\n        layout: {\r\n          'label-col': { span: 6 },\r\n          'wrapper-col': { span: 18 }\r\n        },\r\n        form: this.$form.createForm(this),\r\n        application: {},\r\n        modal: {\r\n          record: null,\r\n          visible: false,\r\n          type: '1',\r\n          application_id: this.id\r\n        },\r\n        columns,\r\n        matterList: []\r\n      }\r\n    },\r\n    mounted () {\r\n      this.getData()\r\n    },\r\n    methods: {\r\n      getData () {\r\n        api.getApplicationMatterList(this.id)\r\n          .then(data => {\r\n            this.matterList = data\r\n          })\r\n        api.getApplication(this.id)\r\n          .then(data => {\r\n            this.application = data\r\n          })\r\n      },\r\n      handleSubmit () {\r\n        this.form.validateFields((error, data) => {\r\n          if (!error) {\r\n            data.title = this.application.title\r\n            api.updateApplication(this.id, data)\r\n              .then(data => {\r\n                this.$notification.success({ message: '成功', description: '修改成功', key: 'SUCCESS' })\r\n              })\r\n          }\r\n        })\r\n      },\r\n      handleClose () {\r\n        this.modal.type = ''\r\n        this.modal.record = null\r\n        this.modal.visible = false\r\n        this.getData()\r\n      },\r\n      handleCreate () {\r\n        this.modal.type = 'create'\r\n        this.modal.record = null\r\n        this.modal.visible = true\r\n      },\r\n      handleEdit (record) {\r\n        this.modal.type = 'edit'\r\n        this.modal.record = record\r\n        this.modal.visible = true\r\n      },\r\n      handleDelete (record) {\r\n        api.deleteApplicationMatter(record.matter_id)\r\n          .then(data => {\r\n            this.$notification.success({ message: '成功', description: '删除成功', key: 'SUCCESS' })\r\n            this.getData()\r\n          })\r\n      },\r\n      parseFloat (f) {\r\n        return parseFloat(f)\r\n      }\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  .item {\r\n    margin-bottom: 10px;\r\n  }\r\n\r\n</style>"]}]}